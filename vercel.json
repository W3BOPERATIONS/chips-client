{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://server-api-one-psi.vercel.app/api/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite"
}
